{
  "name": "tstdl",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prepare": "sed -i '/noImplicitOverride/d' node_modules/@tstdl/base/tsconfig.json",
    "start": "npm run prepare && ng serve",
    "build": "npm run prepare && ng build",
    "pub": "npm run prepare && ng build --configuration production && npm publish dist/tstdl-angular/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.5",
    "@angular/common": "~12.0.5",
    "@angular/compiler": "~12.0.5",
    "@angular/core": "~12.0.5",
    "@angular/forms": "~12.0.5",
    "@angular/platform-browser": "~12.0.5",
    "@angular/platform-browser-dynamic": "~12.0.5",
    "@angular/router": "~12.0.5",
    "@tstdl/base": "^0.64.5",
    "luxon": "1",
    "rxjs": "7",
    "tslib": "2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.5",
    "@angular-devkit/core": "^12.0.5",
    "@angular/cli": "~12.0.5",
    "@angular/compiler-cli": "~12.0.5",
    "@types/luxon": "1",
    "@types/node": "15",
    "@typescript-eslint/eslint-plugin": "4",
    "@typescript-eslint/parser": "4",
    "eslint": "7",
    "ng-packagr": "^12.0.8",
    "typescript": "4.2"
  }
}
